{
  "name": "@reawayjs/vite-plugin-svg-icons",
  "version": "1.0.1",
  "description": "A vite plugin svg-icons",
  "author": "reaway",
  "license": "MIT",
  "homepage": "https://github.com/reaway/vite-plugin-svg-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaway/vite-plugin-svg-icons.git"
  },
  "bugs": {
    "url": "https://github.com/reaway/vite-plugin-svg-icons/issues"
  },
  "keywords": [
    "vite",
    "plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "pnpm unbuild --stub",
    "build": "rimraf dist && pnpm unbuild",
    "example:vue3": "cd examples/vite-vue3 && pnpm run build",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "peerDependencies": {
    "vite": "^3.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "etag": "^1.8.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "svg-baker": "^1.7.0",
    "svgo": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/etag": "^1.8.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "vite": "^3.2.1"
  }
}
